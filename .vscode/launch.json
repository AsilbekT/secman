    "version": "0.2.0",
    "configurations": [
        {
            "name": "Py Debug: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Py Debug: Main file (Encrypt)",
            "type": "debugpy",
            "request": "launch",
            "program": "secman.py",
            "args": [
                "-e"
            ],
            "env": {
                "MKEYPASSWD": "FQRDX23t2Gp0C_BlpgOLG6-uHLxxAN4P2bl4qrp4sBY="
            }
        },
        {
            "name": "Py Debug: Main file (Decrypt)",
            "type": "debugpy",
            "request": "launch",
            "program": "secman.py",
            "args": [
                "-d"
            ],
            "env": {
                "MKEYPASSWD": "FQRDX23t2Gp0C_BlpgOLG6-uHLxxAN4P2bl4qrp4sBY="
            }
        }
    ]
}